<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
		id          = "FashionR"
        version     = "1.0.0" >

    <name>FashionR</name>
    <description>Aplikacja mobilna pozwalajaca na przegladanie dostepnych ofert sklepow odziezowych</description>
    <author>
		Daniel Kucharczyk, Wojciech Wisla, Anna Zukowska
	</author>
	<platform name="android" />
	
	<plugin name="cordova-plugin-device" spec="~1.1.4" />
	<plugin name="cordova-plugin-dialogs" spec="~1.3.1" />
    <plugin name="cordova-plugin-whitelist"/>
    <plugin name="cordova-universal-links-plugin"/>
    <plugin name="cordova-plugin-buildinfo"/>
    <plugin name="cordova-plugin-browsertab"/>
    <plugin name="cordova-plugin-inappbrowser"/>

    <allow-navigation href="*" />
    <allow-intent href="*" />
    <access origin="*" />


</widget>